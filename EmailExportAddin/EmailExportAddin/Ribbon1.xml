<?xml version="1.0" encoding="UTF-8"?>
<customUI xmlns="http://schemas.microsoft.com/office/2009/07/customui" onLoad="Ribbon_Load">
	<ribbon>
		<tabs>
			<tab idMso="TabMail">
				<group id="EmailExport" label="Email Export">					
					<button id="RibbonMenuSaveMailItems"
								size="large"
								  screentip="Shows Email Export window for selected mails"
								  onAction="OnAction"
								   imageMso="SaveAndClose"
								  label="Export mail(s)"
						/>					
				</group>
			</tab>			
		</tabs>		
	</ribbon>
	<contextMenus>		
		<contextMenu idMso="ContextMenuMailItem">
			<button id="ContextMenuSaveMailItem" imageMso="SaveAndClose" label="Export mail" onAction="OnAction" />			
		</contextMenu>
		<contextMenu idMso="ContextMenuMultipleItems">
			<button id="ContextMenuSaveMailItems" imageMso="SaveAndClose" label="Export mails" onAction="OnAction" />
		</contextMenu>
	</contextMenus>
</customUI>

